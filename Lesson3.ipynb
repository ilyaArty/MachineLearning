    "import numpy as np\n",
    "X=np.array([1,2,2,4]);\n",
    "Y=np.array([4,2,1,2]);\n",
    "X=np.sort(X);\n",
    "Y=np.sort(Y);\n",
    "i=0;\n",
    "while i<X.size:\n",
    "    if X[i]==Y[i]:\n",
    "        if i==X.size-1:\n",
    "            print(\"True\");\n",
    "    else:\n",
    "        print(\"False\");\n",
    "        break\n",
    "    i=i+1;"
   ]
